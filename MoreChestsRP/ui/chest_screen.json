{
  "namespace": "chest",
  "chest_label": {
    "$temp_container_title|default": "$container_title",
    "text": "$temp_container_title"
  },
  "barrel_screen": {
    "$temp_container_title|default": "$container_title",
    "is_showing_menu": "$show_close_button",
    "always_accepts_input": "(not $show_close_button)",
    "modifications": [
      {
        "array_name": "variables",
        "operation": "insert_back",
        "value": {
          "requires": "($temp_container_title = 'dCB')",
          "$show_close_button": false
        }
      }
    ]
  }
}
